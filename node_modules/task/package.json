{
  "name": "task",
  "version": "1.3.2",
  "main": "src/index.js",
  "bin": {
    "task": "bin/cmd.js"
  },
  "scripts": {
    "test": "node test | tap-difflet",
    "version-commit": "npm --no-git-tag-version version $(node -pe \"require('./package.json').version.split('-')[0]\")-$(git log -n 1 --pretty=format:'%H')",
    "publish-branch": "npm run version-commit && npm publish --tag $(git rev-parse --abbrev-ref HEAD | sed 's/\\//-/g')",
    "dev": "nodemon test/index.js --ignore files/ | tap-difflet"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/task.git"
  },
  "dependencies": {
    "builder-boy": "*",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "detect-import-require": "^2.0.0",
    "google-closure-compiler-js": "^20170626.0.0",
    "hub.js": "^0.3.12",
    "is-builtin-module": "^1.0.0",
    "node-fetch": "^2.0.0-alpha.5",
    "promise-polyfill": "^6.0.2",
    "read-pkg-up": "^2.0.0",
    "resolve": "^1.3.3",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "stamp": "^4.0.2",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "standard": "^8.6.0",
    "tap-difflet": "^0.7.0",
    "tape": "4.6.3"
  }
}
